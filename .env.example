# Environment Variables Template
# Copy this file to .env.local and fill in your actual values

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Tableau Cloud Configuration  
TABLEAU_SERVER=https://your-site.online.tableau.com
TABLEAU_SITE=your_site_name
TABLEAU_PAT_NAME=your_personal_access_token_name
TABLEAU_PAT_VALUE=your_personal_access_token_value

# Tableau MCP Server Configuration
MCP_SERVER_URL=http://localhost:3001
MCP_AUTH_TOKEN=your_mcp_auth_token

# Application Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development

# Database (if needed for caching)
DATABASE_URL=your_database_url_here

# Optional: For advanced features
ANTHROPIC_API_KEY=your_anthropic_key_if_needed
HUGGING_FACE_API_KEY=your_hf_key_if_needed